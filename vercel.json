{
  "crons": [
    {
      "path": "/api/cron/check-expired-subscriptions",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/check-task-notifications",
      "schedule": "*/2 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}
